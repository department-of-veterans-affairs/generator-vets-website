const E2eHelpers = require('<%= subFolder %>../e2e/e2e-helpers');
const manifest = require('<%= subFolder %>../../src/js/<%= folderName %>/manifest.json');
const Timeouts = require('<%= subFolder %>../e2e/timeouts.js');

module.exports = E2eHelpers.createE2eTest((client) => {
  client
    .url(`${E2eHelpers.baseUrl}<%= rootUrl %>`)
    .waitForElementVisible('body', Timeouts.normal)
    .axeCheck('.main');

  client.end();
});

module.exports['@disabled'] = !manifest.production;
