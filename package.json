{
  "name": "@department-of-veterans-affairs/generator-vets-website",
  "version": "3.18.1",
  "description": "Generate a React app for vets-website",
  "homepage": "",
  "author": {
    "name": "DSVA"
  },
  "files": [
    "generators",
    "utils",
    "lib"
  ],
  "main": "generators/app/index.js",
  "keywords": [
    "VA",
    "vets.gov",
    "dsva",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "8.28.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-xo": "0.43.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.2",
    "lint-staged": "13.0.4",
    "mocha": "10.8.2",
    "prettier": "2.8.1"
  },
  "engines": {
    "node": "^14.15.0"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "yeoman-generator": "5.7.0",
    "yosay": "2.0.2"
  },
  "scripts": {
    "prepublishOnly": "npm audit --audit-level=high",
    "lint": "eslint .",
    "test": "mocha 'test/**/*.spec.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "overrides": [
      {
        "files": [
          "test/**/*.js",
          "tests/**/*.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90,
          "trailingComma": "all"
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "resolutions": {
    "**/axios": "^0.21.1",
    "**/glob-parent": "5.1.2",
    "**/ansi-regex": "5.0.1",
    "**/minimist": "1.2.6"
  },
  "repository": "department-of-veterans-affairs/generator-vets-website",
  "license": "CC0-1.0"
}
