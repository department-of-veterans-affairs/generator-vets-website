{
  "name": "@department-of-veterans-affairs/generator-vets-website",
  "version": "3.3.5",
  "description": "Generate a React app for vets-website",
  "homepage": "",
  "author": {
    "name": "DSVA"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "VA",
    "vets.gov",
    "dsva",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.1",
    "prettier": "^2.0.5",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "scripts": {
    "prepublishOnly": "npm audit",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90,
          "trailingComma": "all"
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "department-of-veterans-affairs/generator-vets-website",
  "license": "CC0-1.0"
}
