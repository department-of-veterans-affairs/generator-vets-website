{
  "name": "@department-of-veterans-affairs/generator-vets-website",
  "version": "3.4.0",
  "description": "Generate a React app for vets-website",
  "homepage": "",
  "author": {
    "name": "DSVA"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "VA",
    "vets.gov",
    "dsva",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-xo": "^0.31.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^2.6.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "yeoman-generator": "^4.11.0",
    "yosay": "^2.0.1"
  },
  "scripts": {
    "prepublishOnly": "npm audit",
    "pretest": "eslint .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90,
          "trailingComma": "all"
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "department-of-veterans-affairs/generator-vets-website",
  "license": "CC0-1.0"
}
